<script type="text/javascript">
    $(document).ready(function () {
        $("#names").autocomplete({
            source: function (request, response) {
                $.ajax({
                    url: "srch",
                    type: "POST",
                    dataType: "json",
                    data: { term: request.term },
                    success: function (data) {
                        response($.map(data, function (item) {
                            return { label: item.Fname, value: item.Fname };
                        }))
                    }
                })
            },
            messages: {
                noResults: "", results: ""
            }
        });
    })
</script>



